FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Vapor diffusion number
DX          DX  [ 0 2 -1 0 0 0 0 ] 2.61e-5;

// Turbulent Schmidt number
Sct         Sct [ 0 0  0 0 0 0 0 ] 1.0;

